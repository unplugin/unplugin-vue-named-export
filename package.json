{
  "name": "unplugin-vue-named-export",
  "version": "0.7.1",
  "packageManager": "pnpm@10.5.2",
  "description": "Named export for Vue SFC.",
  "type": "module",
  "keywords": [
    "unplugin",
    "rollup",
    "vite",
    "esbuild",
    "webpack"
  ],
  "license": "MIT",
  "homepage": "https://github.com/unplugin/unplugin-vue-named-export#readme",
  "bugs": {
    "url": "https://github.com/unplugin/unplugin-vue-named-export/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unplugin/unplugin-vue-named-export.git"
  },
  "author": "三咲智子 Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./farm": {
      "require": "./dist/farm.cjs",
      "import": "./dist/farm.js"
    },
    "./rspack": {
      "require": "./dist/rspack.cjs",
      "import": "./dist/rspack.js"
    },
    "./rolldown": {
      "require": "./dist/rolldown.cjs",
      "import": "./dist/rolldown.js"
    },
    "./volar": {
      "require": "./dist/volar.cjs",
      "import": "./dist/volar.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit && vue-tsc --noEmit -p tests/fixtures/tsconfig.json",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@vue/language-core": "^2.2.8",
    "ast-kit": "^1.4.2",
    "magic-string-ast": "^0.8.0",
    "muggle-string": "^0.4.1",
    "unplugin": "^2.2.0",
    "unplugin-utils": "^0.2.4"
  },
  "devDependencies": {
    "@babel/types": "^7.26.9",
    "@sxzz/eslint-config": "^5.2.0",
    "@sxzz/prettier-config": "^2.2.1",
    "@sxzz/test-utils": "^0.5.1",
    "@types/node": "^22.13.9",
    "bumpp": "^10.0.3",
    "change-case": "^5.4.4",
    "eslint": "^9.21.0",
    "prettier": "^3.5.3",
    "tsdown": "^0.6.4",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unplugin-oxc": "^0.2.6",
    "unplugin-vue": "^6.0.1",
    "vite": "^6.2.0",
    "vitest": "^3.0.7",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "prettier": "@sxzz/prettier-config"
}
