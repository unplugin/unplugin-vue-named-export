{
  "name": "unplugin-vue-named-export",
  "version": "0.6.0",
  "packageManager": "pnpm@9.15.4",
  "description": "Named export for Vue SFC.",
  "type": "module",
  "keywords": [
    "unplugin",
    "rollup",
    "vite",
    "esbuild",
    "webpack"
  ],
  "license": "MIT",
  "homepage": "https://github.com/unplugin/unplugin-vue-named-export#readme",
  "bugs": {
    "url": "https://github.com/unplugin/unplugin-vue-named-export/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unplugin/unplugin-vue-named-export.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./volar": {
      "require": "./dist/volar.cjs",
      "import": "./dist/volar.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.4",
    "@vue/language-core": "^2.2.0",
    "ast-kit": "^1.4.0",
    "magic-string-ast": "^0.7.0",
    "muggle-string": "^0.4.1",
    "unplugin": "^2.1.2"
  },
  "devDependencies": {
    "@babel/types": "^7.26.7",
    "@sxzz/eslint-config": "^5.0.0",
    "@sxzz/prettier-config": "^2.1.1",
    "@types/node": "^22.12.0",
    "@vue-macros/test-utils": "^1.7.1",
    "bumpp": "^10.0.1",
    "change-case": "^5.4.4",
    "eslint": "^9.19.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.4",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "prettier": "@sxzz/prettier-config"
}
