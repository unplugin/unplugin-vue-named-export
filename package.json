{
  "name": "unplugin-vue-named-export",
  "version": "0.8.1",
  "packageManager": "pnpm@10.10.0",
  "description": "Named export for Vue SFC.",
  "type": "module",
  "keywords": [
    "unplugin",
    "rollup",
    "vite",
    "esbuild",
    "webpack"
  ],
  "license": "MIT",
  "homepage": "https://github.com/unplugin/unplugin-vue-named-export#readme",
  "bugs": {
    "url": "https://github.com/unplugin/unplugin-vue-named-export/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unplugin/unplugin-vue-named-export.git"
  },
  "author": "三咲智子 Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./vite": "./dist/vite.js",
    "./webpack": "./dist/webpack.js",
    "./rollup": "./dist/rollup.js",
    "./esbuild": "./dist/esbuild.js",
    "./farm": "./dist/farm.js",
    "./rspack": "./dist/rspack.js",
    "./rolldown": "./dist/rolldown.js",
    "./volar": "./dist/volar.js",
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit && vue-tsc --noEmit -p tests/fixtures/tsconfig.json",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@vue/language-core": "^2.2.10",
    "ast-kit": "^2.0.0",
    "magic-string-ast": "^0.9.1",
    "muggle-string": "^0.4.1",
    "unplugin": "^2.3.2"
  },
  "devDependencies": {
    "@babel/types": "^7.27.1",
    "@sxzz/eslint-config": "^7.0.0",
    "@sxzz/prettier-config": "^2.2.1",
    "@sxzz/test-utils": "^0.5.6",
    "@types/node": "^22.15.17",
    "bumpp": "^10.1.0",
    "change-case": "^5.4.4",
    "eslint": "^9.26.0",
    "prettier": "^3.5.3",
    "tsdown": "^0.11.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "unplugin-oxc": "^0.3.7",
    "unplugin-vue": "^6.2.0",
    "vite": "^7.0.0",
    "vitest": "^3.1.3",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "prettier": "@sxzz/prettier-config"
}
